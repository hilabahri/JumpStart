        $(document).ready(function () {

            $('.popup-with-form').magnificPopup({
                type: 'inline',
                preloader: false,
                focus: '#name',

                // When elemened is focused, some mobile browsers in some cases zoom in
                // It looks not nice, so we disable it:
                callbacks: {
                    beforeOpen: function () {
                        if ($(window).width() < 700) {
                            this.st.focus = false;
                        } else {
                            this.st.focus = '#name';
                        }
                    }
                }
            });

            var requestsTable = $('#RequestsTable').dataTable({
                "ajax": "AcquireData",
                "columns": [
               { "data": "Name" },
               { "data": "Date" },
               { "data": "Collected" },
               { "data": "Goal" }
                ],
                "fnRowCallback": function (nRow, aData, isDisplayIndex, iDisplayFullIndex) {
                    var shouldPop = false;
                    var sum = aData.Goal - aData.Collected;
                    if (aData.Date === "") {
                        $(nRow).css('background-color', 'darkgray');
                    }
                    else if (sum <= 0) {
                        $(nRow).css('background-color', 'chartreuse');
                        shouldPop = true;

                    }

                    if (shouldPop) {
                        $.magnificPopup.open({
                            items: {
                                src: '#test-form'
                            },
                            type: 'inline'

                            // You may add options here, they're exactly the same as for $.fn.magnificPopup call
                            // Note that some settings that rely on click event (like disableOn or midClick) will not work here
                        }, 0);
                    }
                }

            }).ajaxComplete(function () {
               
                }
            );

        });
